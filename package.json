{
  "name": "katas-nodejs",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf dist",
    "purge": "npm run clear && rm -rf node-modules",
    "reset": "npm run purge && rm package-lock.json && npm i",
    "build:all": "npx nx run-many --all --target=build --parallel",
    "lint:all": "nx workspace-lint && npx nx run-many --all --target=lint --parallel",
    "test:all": "npx nx run-many --all --target=test --parallel",
    "format:check:all": "npx nx run-many --all --target=format:check --parallel",
    "upgrade": "npx nx migrate latest"
  },
  "engines": {
    "node": ">= 16.15.0"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/js": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "28.1.0",
    "@types/node": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.17.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "28.1.0",
    "nx": "14.1.9",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.6.0"
  }
}
